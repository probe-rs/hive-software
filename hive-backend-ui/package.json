{
  "name": "hive-backend",
  "version": "0.1.0",
  "scripts": {
    "build": "run-p check \"build-only {@}\" --",
    "build-only": "vite build",
    "gql-types": "cargo run --manifest-path ../monitor/Cargo.toml -- gql-schema > schema.gql && graphql-codegen",
    "test:unit": "vitest --environment jsdom",
    "test:e2e": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress open'",
    "lint": "prettier -c . && eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "dev": "vite",
    "preview": "vite preview --port 5050",
    "test:e2e:ci": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress run'",
    "check": "vue-tsc --build --force",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@apollo/client": "^3.9.2",
    "@mdi/font": "7.4.47",
    "@vue/apollo-composable": "^4.2.2",
    "@vue/apollo-util": "^4.2.2",
    "ansi-colors": "^4.1.3",
    "base-64": "^1.0.0",
    "generate-password-browser": "^1.1.0",
    "graphql": "^16.12.0",
    "js-cookie": "^3.0.5",
    "konva": "^10.2.0",
    "pinia": "^3.0.4",
    "pinia-plugin-persistedstate": "^4.7.1",
    "vite-plugin-vuetify": "^2.1.3",
    "vue": "^3.5.27",
    "vue-konva": "^3.3.0",
    "vue-router": "^4.6.4",
    "vue3-ace-editor": "^2.2.4",
    "vuetify": "^3.11.7",
    "xterm": "^5.3.0",
    "xterm-addon-fit": "^0.8.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/client-preset": "^4.2.2",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@rushstack/eslint-patch": "^1.7.2",
    "@tsconfig/node20": "^20.1.2",
    "@types/base-64": "^1.0.0",
    "@types/js-cookie": "^3.0.5",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.11.17",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "cypress": "^13.6.4",
    "eslint": "^8.56.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-vue": "^9.21.1",
    "jsdom": "^24.0.0",
    "npm-run-all2": "^6.1.1",
    "prettier": "^3.8.1",
    "sass": "^1.70.0",
    "sass-loader": "^14.1.0",
    "start-server-and-test": "^2.1.3",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^1.2.2",
    "vue-cli-plugin-vuetify": "^2.5.8",
    "vue-tsc": "^3.2.2",
    "vuetify-loader": "^2.0.0-alpha.0"
  }
}
